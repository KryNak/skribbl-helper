               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="200.0" prefWidth="800.0">
               <children>
                  <Button id="btn3" mnemonicParsing="false" prefHeight="70.0" prefWidth="300.0" text="Kopiuj" onAction="#copyText" />
               </children>
